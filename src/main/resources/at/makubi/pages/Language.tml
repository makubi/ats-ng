<html t:type="layout" title="Import"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

        <t:form t:id="addLanguageForm">
            <t:errors/>
            <t:label for="code" />
            <br />
            <input t:type="TextField" t:id="code" t:validate="required" size="6" />
            <br />
            <br/>
            <t:label for="name" />
            <br />
            <input t:type="TextField" t:id="name" t:validate="required" size="10" />
            <br />
            <br />
            <input type="submit" value="Upload"/>
        </t:form>
</html>
